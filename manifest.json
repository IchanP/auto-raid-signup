{
  "name": "Auto Sign",
  "description": "Auto Raid Signup through Discord",
  "version": "0.4.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "minimum_chrome_version": "92",
  "action": {
    "default_title": "Open Settings"
  },
  "permissions": [
    "alarms",
    "tabs",
    "scripting",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}